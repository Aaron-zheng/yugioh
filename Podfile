source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '8.0'
use_frameworks!

target '游戏王卡牌' do
  # your other pod
  # pod 'Wilddog/Sync'

  ## 加载图片
  pod 'Kingfisher', '~> 3.0'

  ## 浮动按钮
  pod 'Floaty', '~> 3.0.0'

  ## 图片放大
  pod 'Agrume', :git => 'https://github.com/JanGorman/Agrume.git'

  ## 评论功能
  pod 'LeanCloud'

  ## 微信接口
  pod 'WechatOpenSDK'
end