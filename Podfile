source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '9.0'
use_frameworks!

target '游戏王卡牌' do

  ## 加载图片
  pod 'Kingfisher', '~> 4.0'

  ## 图片放大
  pod 'Agrume', :git => 'https://github.com/JanGorman/Agrume.git'

  ## 微信接口
  pod 'WechatOpenSDK'

  ## sqlite工具
  pod 'SQLite.swift', '~> 0.11.4'

  ## 评论功能
  pod 'LeanCloud', '~> 11.0.0'
  
  ## 浮动按钮
  pod 'Floaty', '~> 4.0.0'


  ############## 注释不用的 ##############
  
  ## 野狗实时通讯
  ## pod 'Wilddog/Sync'

  ## 添加用户统计工具
  ## pod 'Firebase/Core'


  
end