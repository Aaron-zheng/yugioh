source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '9.0'
use_frameworks!

target '游戏王卡牌' do
  # your other pod

  ## 加载图片
  pod 'Kingfisher', '~> 3.0'

  ## 浮动按钮
  pod 'Floaty', '~> 3.0.0'

  ## 图片放大
  pod 'Agrume', :git => 'https://github.com/JanGorman/Agrume.git'

  ## 评论功能
  pod 'LeanCloud'

  ## 微信接口
  pod 'WechatOpenSDK'

  ## 野狗实时通讯
  pod 'Wilddog/Sync'

  ## 添加用户统计工具
  pod 'Firebase/Core'

  ## sqlite工具
  pod 'SQLite.swift', '~> 0.11.3'
end